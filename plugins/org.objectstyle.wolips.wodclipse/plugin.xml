<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

 	<extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.objectstyle.wolips.wodclipse.wod.WodEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            default="false"
            extensions="wod"
            icon="icons/WODEditor.gif"
            id="org.objectstyle.wolips.wodclipse.wod.WodEditor"
            name="Wod Editor">
			<contentTypeBinding
               contentTypeId="org.objectstyle.wolips.editors.wod"
            /> 
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.objectstyle.wolips.wodclipse.preferences.WodEditorPreferencePage"
            category="org.objectstyle.wolips.preferences.PreferencesPage"
            id="org.objectstyle.wolips.wodclipse.preferences.WodEditorPreferencePage"
            name="Wod Editor Preferences"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.objectstyle.wolips.wodclipse.preferences.PreferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            description="Editing Wod Files Context"
            id="org.objectstyle.wolips.wodclipse.wodEditorScope"
            name="Editing Wod Files"
            parentId="org.eclipse.ui.textEditorScope"/>
   </extension>
		
 <extension
       point="org.objectstyle.wolips.builders">
    <builder
          class="org.objectstyle.wolips.wodclipse.builder.WodBuilder"
          id="org.objectstyle.wolips.wodclipse.builder.WodBuilder"
          name="Wod Builder"/>
 </extension>
</plugin>
