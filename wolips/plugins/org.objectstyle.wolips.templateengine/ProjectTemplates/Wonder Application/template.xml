<?xml version="1.0" encoding="UTF-8"?>
<template name = "Wonder Application">
	<inputs>
		<input name = "linkToWonderProjects" type = "Boolean">
			<question>Link to Wonder Eclipse Projects?</question>
			<default>false</default>
		</input>
		<input name = "linkToWonderFrameworks" type = "Boolean">
			<question>Link to Installed Wonder Frameworks?</question>
			<default>true</default>
		</input>
		<input name = "basePackage" type = "Package">
			<question>Base Package?</question>
			<default>your.app</default>
		</input>
		<input name = "componentsPackage" type = "Package">
			<question>Components Package?</question>
			<default>your.app.components</default>
		</input>
	</inputs>
</template>
