<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

<!-- =================================================================================== -->
<!-- Extension: Adapters                                                                 -->
<!-- =================================================================================== -->
   <extension point="org.eclipse.core.runtime.adapters">
       <factory class="org.objectstyle.wolips.datasets.adaptable.IProjectAdapterFactory"
           adaptableType="org.eclipse.core.resources.IProject">
           <adapter type="org.objectstyle.wolips.datasets.adaptable.Project"/>
       </factory>
       <factory class="org.objectstyle.wolips.datasets.adaptable.IJavaProjectAdapterFactory"
           adaptableType="org.eclipse.jdt.core.IJavaProject">
           <adapter type="org.objectstyle.wolips.datasets.adaptable.JavaProject"/>
       </factory>
       <factory class="org.objectstyle.wolips.datasets.adaptable.IFolderAdapterFactory"
           adaptableType="org.eclipse.core.resources.IFolder">
           <adapter type="org.objectstyle.wolips.datasets.adaptable.LProjFolder"/>
       </factory>
   </extension>
   <extension
         id="pbproject.warning"
         name="PBProject Generation"
         point="org.eclipse.core.resources.markers">
      <super type="org.eclipse.core.resources.taskmarker"/>
      <persistent value="false"/>
   </extension>
   
</plugin>
