<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.objectstyle.woproject</groupId>
		<artifactId>woproject-all-parent</artifactId>
		<version>2.0.16-SNAPSHOT</version>
		<relativePath>../../../pom.xml</relativePath>
	</parent>
	<groupId>org.objectstyle.wolips</groupId>
	<artifactId>base-for-plugins</artifactId>
	<packaging>jar</packaging>
	<name>Base For Plug-ins</name>
	<description>Contains some base classes for plug-ins.</description>
	<build>
		<sourceDirectory>java</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<!--
						WARNING! This configuration was made to avoid dependencies on Eclipse classes.
						The artifact generated by this build is not complete. 
						It contains only the required classes to build woenvironment
						and woproject-ant-tasks modules.
					-->
					<includes>
						<include>org/objectstyle/wolips/baseforplugins/plist/*</include>
					</includes>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>